<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkApplicationWindow" id="main_window">
    <property name="title" translatable="yes">P2PC</property>
    <property name="default-width">1920</property>
    <property name="default-height">1080</property>
    <child>
      <object class="GtkGrid" id="main_grid">
        <child>
          <object class="GtkLabel" id="chat_label">
            <property name="label">Chat</property>
            <property name="vexpand">true</property>
            <property name="valign">fill</property>
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="chat_entry">
            <property name="hexpand">true</property>
            <property name="halign">fill</property>
            <layout>
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
